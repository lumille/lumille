{
    "name": "lumille/lumille",
    "description": "Minimalist Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Frederic KOLLER",
            "email": "koller.f@gmail.com"
        }
    ],
    "require": {
        "eftec/bladeone": "3.31",
        "fzaninotto/faker": "^1.8",
        "illuminate/support": "^5.8",
        "symfony/http-foundation": "^4.3",
        "lumille/routing": "^0.1",
        "pimple/pimple": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Lumille\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0"
        }
    },
    "minimum-stability": "dev",
    "replace": {
        "lumille/routing": "self.version"
    }

}
